<item/work1> a crm:E22_Man-Made_Object ;
    dcterms:creator "person/ANDRECO"^^xsd:string ;
    schema:dateCreated "2012"^^xsd:gYear ;
    crm:P102_has_title <title/work1> ;
    crm:P129_is_about "subject/nature "^^xsd:string ;
    crm:P138_represents <theme/environment> ;
    crm:P1_is_identified_by <identifier/work1> ;
    crm:P2_has_type "<http://vocab.getty.edu/aat/300056477>"^^xsd:string ;
    crm:P3_has_note "The work represents a large tree, inspired by the Philosophical Tree of the Alchemists of the fourteenth century. A large egg and a crystal also appear in the painting, symbols representing the transition from organic to inorganic matter. This wall painting is made with a photochemical paint that reduces the pollutant, such as NOx (nitrogen monoxide), improving air quality.The tree painted on the walls works like a natural tree that cleans\" the air and makes a small contribution to a better urban environment.\""^^xsd:string ;
    crm:P44_has_condition <condition/visible> ;
    crm:P46_forms_part_of <support/wall> ;
    crm:P53_has_former_or_current_location <place/work1> .

<item/work10> a crm:E22_Man-Made_Object ;
    dcterms:creator "person/Bastardilla "^^xsd:string ;
    schema:dateCreated "2014"^^xsd:gYear ;
    crm:P102_has_title <title/work10> ;
    crm:P129_is_about "subject/human figure"^^xsd:string ;
    crm:P138_represents <theme/social_issue> ;
    crm:P1_is_identified_by <identifier/work10> ;
    crm:P2_has_type "<http://vocab.getty.edu/aat/300056477>"^^xsd:string ;
    crm:P3_has_note "\"This untitled mural, created by Colombian street artist Bastardilla, is located on the exterior wall of the Liceo Scientifico Augusto Righi branch in Via Tolmino 9, Bologna, Italy. It is part of the FRONTIER street art project. The mural depicts a circle of \"matchstick children\" sitting together, collectively igniting a flame—a symbol of the spread of knowledge and the power of collective wisdom. These \"matchstick children\" represent the awakening of pre-adolescent thought, conveying the immense potential contained within young minds.\""^^xsd:string ;
    crm:P44_has_condition <condition/visible> ;
    crm:P46_forms_part_of <support/wall> ;
    crm:P53_has_former_or_current_location <place/work10> .

<item/work11> a crm:E22_Man-Made_Object ;
    dcterms:creator "person/Etnik "^^xsd:string ;
    schema:dateCreated "2012"^^xsd:gYear ;
    crm:P102_has_title <title/work11> ;
    crm:P129_is_about "subject/geometric shapes"^^xsd:string ;
    crm:P138_represents <theme/environment> ;
    crm:P1_is_identified_by <identifier/work11> ;
    crm:P2_has_type "<http://vocab.getty.edu/aat/300056477>"^^xsd:string ;
    crm:P3_has_note "\"Represent the precarious balance of the human being and the speed of the contemporary world. The urban agglomerations are broken down and overlapped and the geometrically extreme architecture becomes the emblem of the concept of \"urban cage\", a sort of poetics of the artist who sees metropolitan centers as places where man is trapped and which he criticizes with the use of strong colors and a bright and contrasting chromatic range, in wall paintings in which sometimes the identification of the buildings is almost impossible.\""^^xsd:string ;
    crm:P44_has_condition <condition/visible> ;
    crm:P46_forms_part_of <support/wall> ;
    crm:P53_has_former_or_current_location <place/work11> .

<item/work12> a crm:E22_Man-Made_Object ;
    dcterms:creator "person/Blu "^^xsd:string ;
    schema:dateCreated "2013"^^xsd:gYear ;
    crm:P102_has_title <title/work12> ;
    crm:P129_is_about "subject/city"^^xsd:string ;
    crm:P138_represents <theme/social_issue_> ;
    crm:P1_is_identified_by <identifier/work12> ;
    crm:P2_has_type "<http://vocab.getty.edu/aat/300056477>"^^xsd:string ;
    crm:P3_has_note "\"This large-scale mural by the Italian street artist Blu, painted on the facade of the XM24 social center in Bologna’s Bolognina district, depicts a symbolic battle between two opposing forces within the city: a wealthy, ruthless side represented by figures hurling mortadellas, and a grassroots, justice-driven side throwing pumpkins and watermelons. Drawing inspiration from The Lord of the Rings, the artwork reflects over fifteen years of social movements. With a dramatic contrast between intricate black-and-white architectural ruins and vibrant, color-filled zones, the mural serves as both a critique of gentrification and a powerful visual narrative of urban resistance.\""^^xsd:string ;
    crm:P44_has_condition <condition/destroyed> ;
    crm:P46_forms_part_of <support/wall> ;
    crm:P53_has_former_or_current_location <place/work12> .

<item/work2> a crm:E22_Man-Made_Object ;
    dcterms:creator "person/Bastardilla"^^xsd:string,
        "person/Ericailcane"^^xsd:string ;
    schema:dateCreated "2020"^^xsd:gYear ;
    crm:P102_has_title <title/work2> ;
    crm:P129_is_about "subject/animal"^^xsd:string,
        "subject/human figure"^^xsd:string ;
    crm:P138_represents <theme/social_issue> ;
    crm:P1_is_identified_by <identifier/work2> ;
    crm:P2_has_type "<http://vocab.getty.edu/aat/300056477>"^^xsd:string ;
    crm:P3_has_note "\"One one side there are some female figures throwing purple balloons using a surgical mask as a slingshot . On the other side there are mice sheltering from those balloons behind coins, holding cotton buds\""^^xsd:string ;
    crm:P44_has_condition <condition/visible> ;
    crm:P46_forms_part_of <support/gate>,
        <support/wall> ;
    crm:P53_has_former_or_current_location <place/work2> .

<item/work3> a crm:E22_Man-Made_Object ;
    dcterms:creator "person/Collettivo FX"^^xsd:string ;
    schema:dateCreated "2020"^^xsd:gYear ;
    crm:P102_has_title <title/work3> ;
    crm:P129_is_about "subject/human figure"^^xsd:string ;
    crm:P138_represents <theme/history>,
        <theme/social_issue> ;
    crm:P1_is_identified_by <identifier/work3> ;
    crm:P2_has_type "<http://vocab.getty.edu/aat/300056477>"^^xsd:string ;
    crm:P3_has_note "\"Woman crying big tears. In one tear there are the bodies of the victims, in the other one the clock fixed at the hour of the attack. \""^^xsd:string ;
    crm:P44_has_condition <condition/visible> ;
    crm:P46_forms_part_of <support/wall> ;
    crm:P53_has_former_or_current_location <place/work3> .

<item/work4> a crm:E22_Man-Made_Object ;
    dcterms:creator "person/Orticanoodles"^^xsd:string ;
    schema:dateCreated "2017"^^xsd:gYear ;
    crm:P102_has_title <title/work4> ;
    crm:P129_is_about "subject/human figure"^^xsd:string ;
    crm:P138_represents <theme/history>,
        <theme/social_issue> ;
    crm:P1_is_identified_by <identifier/work4> ;
    crm:P2_has_type "<http://vocab.getty.edu/aat/300056477>"^^xsd:string ;
    crm:P3_has_note "\"The artwork depicts the face of Irma Bandiera, a young partisan courier who was tortured and killed by the fascists in 1944, with a proud gaze and vibrant colors emerging from the wall. The graffiti aims to restore visibility and dignity to her figure, transforming the site into a place of public memory and visual resistance.\""^^xsd:string ;
    crm:P44_has_condition <condition/visible> ;
    crm:P46_forms_part_of <support/wall> ;
    crm:P53_has_former_or_current_location <place/work4> .

<item/work5> a crm:E22_Man-Made_Object ;
    dcterms:creator "person/Luis Gutierrez"^^xsd:string ;
    schema:dateCreated "1998"^^xsd:gYear ;
    crm:P102_has_title <title/work5> ;
    crm:P129_is_about "subject/human figure"^^xsd:string ;
    crm:P138_represents <theme/history> ;
    crm:P1_is_identified_by <identifier/work5> ;
    crm:P2_has_type "<http://vocab.getty.edu/aat/300056477>"^^xsd:string ;
    crm:P3_has_note "\"This mural is an example of the strength of the drawing, sensual and sharp, with which the Latin American artist has created a personal mythology, a balance between the real and the imaginary, between rituals and archaic myths, human legends and characters such as Fidel Castro and even Caravaggio.\""^^xsd:string ;
    crm:P44_has_condition <condition/restored>,
        <condition/visible> ;
    crm:P46_forms_part_of <support/wall> ;
    crm:P53_has_former_or_current_location <place/work5> .

<item/work6> a crm:E22_Man-Made_Object ;
    dcterms:creator "person/Honet"^^xsd:string ;
    schema:dateCreated "2012"^^xsd:gYear ;
    crm:P102_has_title <title/work6> ;
    crm:P129_is_about "subject/animal"^^xsd:string ;
    crm:P138_represents <theme/history>,
        <theme/social_issue> ;
    crm:P1_is_identified_by <identifier/work6> ;
    crm:P2_has_type "<http://vocab.getty.edu/aat/300056477>"^^xsd:string ;
    crm:P3_has_note "\"A large elephant with a castle on its back is being dragged through the air by colourful balloons, three black birds appear ready to poke holes in them. According to the artist, he was inspired for this mural by a huge fountain in the form of an elephant that Napoleon wanted to place in Bastille Square in Paris to celebrate his victories and power. Work stopped after the defeat at Waterloo. The elephant has always been a symbol of strength. In this case, although enormous in size, it is so light that it can be carried by a handful of balloons that can be pierced by a simple bird in flight. The reference to power (even regal power) that can be as great as it is fragile is obvious: Honet nevertheless represents this theme in a delicate and cheerful manner.\""^^xsd:string ;
    crm:P44_has_condition <condition/visible> ;
    crm:P46_forms_part_of <support/wall> ;
    crm:P53_has_former_or_current_location <place/work6> .

<item/work7> a crm:E22_Man-Made_Object ;
    dcterms:creator "person/ALice Pasquini"^^xsd:string ;
    schema:dateCreated "2018"^^xsd:gYear ;
    crm:P102_has_title <title/work7> ;
    crm:P129_is_about "subject/human figure"^^xsd:string ;
    crm:P138_represents <theme/identity> ;
    crm:P1_is_identified_by <identifier/work7> ;
    crm:P2_has_type "<http://vocab.getty.edu/aat/300056477>"^^xsd:string ;
    crm:P3_has_note "\"Alice depicted a girl with a bunch of colourful flowers in her hand, hiding part of her face behind the curtain, recalling that street art “is an art that stirs up controversy” and during her career there has certainly been no shortage of problems, but acknowledging that after all “it is the risk of signing one's name”.\""^^xsd:string ;
    crm:P44_has_condition <condition/visible> ;
    crm:P46_forms_part_of <support/gate> ;
    crm:P53_has_former_or_current_location <place/work7> .

<item/work8> a crm:E22_Man-Made_Object ;
    dcterms:creator "person/Nemo"^^xsd:string ;
    schema:dateCreated "2017"^^xsd:gYear ;
    crm:P102_has_title <title/work8> ;
    crm:P129_is_about "subject/human figure"^^xsd:string ;
    crm:P138_represents <theme/social_issue_> ;
    crm:P1_is_identified_by <identifier/work8> ;
    crm:P2_has_type "<http://vocab.getty.edu/aat/300056477>"^^xsd:string ;
    crm:P3_has_note "\"The flaccid, grotesque and soft appearance with which Nemo's depicts its characters is ultimately intended as a critique of contemporary society, of the perfection and apparent immortality into which it projects us, heedless of the decadence of the human being and his contradictions that for thousands of years have led him to repeat the same mistakes over and over again.\""^^xsd:string ;
    crm:P44_has_condition <condition/visible> ;
    crm:P46_forms_part_of <support/wall> ;
    crm:P53_has_former_or_current_location <place/work8> .

<item/work9> a crm:E22_Man-Made_Object ;
    dcterms:creator "person/Dado "^^xsd:string,
        "person/Mambo"^^xsd:string ;
    schema:dateCreated "2018"^^xsd:gYear ;
    crm:P102_has_title <title/work9> ;
    crm:P129_is_about "subject/geometric shapes"^^xsd:string ;
    crm:P138_represents <theme/environment> ;
    crm:P1_is_identified_by <identifier/work9> ;
    crm:P2_has_type "<http://vocab.getty.edu/aat/300056477>"^^xsd:string ;
    crm:P3_has_note "\"This untitled mural was created in 2018 by Italian street artists Dado (Alessandro Ferri) and Mambo (Elvis Mambo Pregnolato). It is located on the exterior wall of the CISL (Italian Confederation of Workers' Trade Unions) headquarters on Via Milazzo in Bologna. The large-scale mural—approximately 50 meters wide and 12 meters high—uses red and green as dominant colors, echoing the identity of the union. The artwork features two women wearing safety helmets and a man with a mask and helmet, symbolizing labor and highlighting the importance of workers’ safety. Blending abstract style with figurative elements, the mural pays tribute to the working class and reflects the artists' commitment to merging street art with social themes.\""^^xsd:string ;
    crm:P44_has_condition <condition/visible> ;
    crm:P46_forms_part_of <support/wall> ;
    crm:P53_has_former_or_current_location <place/work9> .

